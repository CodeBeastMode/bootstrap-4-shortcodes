{{#sections}}
<h1>{{title}}</h1>
{{#items}}
<section id="{{id}}">
<h2>{{title}}</h2>
{{#note}}<p>{{{note}}}</p>{{/note}}
{{#examples}}
	{{#note}}<p>{{{note}}}</p>{{/note}}
	<div class="card">
		<div class="card-block">
			<pre><code>{{code}}</code></pre>
		</div>
		<div class="card-footer">
			{{description}}
			<button data-path="placeholder" data-dismiss="modal" class="btn btn-primary btn-sm float-xs-right insert-code">Insert Example</button>
		</div>
	</div> <!--/card -->
{{/examples}}

{{#shortcodes}}
<h3>[{{name}}] parameters</h3>
{{#note}}
<div class="alert alert-warning"><strong>Note:</strong> {{{note}}}</div>
{{/note}}
<table class="table table-striped">
	<thead>
		<tr>
			<th>Parameter</th>
			<th>Description</th>
			<th>Required</th>
			<th>Values</th>
			<th>Default</th>
		</tr>
	</thead>
	<tbody>
		{{#parameters}}
		<tr>
			<td>{{name}}</td>
			<td>{{description}}</td>
			<td>{{required}}</td>
			<td>{{values}}</td>
			<td>{{default}}</td>
		</tr>
		{{/parameters}}
		<tr>
			<td>class</td>
			<td>Any extra classes you want to add.</td>
			<td>optional</td>
			<td>any text</td>
			<td></td>
		</tr>
		<tr>
			<td>data</td>
			<td>Data attribute and value pairs separated by a comma. Pairs separated by pipe.</td>
			<td>optional</td>
			<td>any text</td>
			<td></td>
		</tr>
	</tbody>
</table>
{{/shortcodes}}
</section>
{{/items}}
{{/sections}}
